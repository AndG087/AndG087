while True:
  if gpio.input(button1) and gpio.input(button2) == gpio.HIGH:
    confi3()
  elif gpio.input(button1) == gpio.HIGH:
    conf7()
  elif gpio.input(button2) == gpio.HIGH:
    conf11()
   
