while True:
    conf3()
    time.sleep(1)
    conf7()
    time.sleep(1)
    conf11()
    time.sleep(1)
    conf15()
    time.sleep(1)
    conf19()
    time.sleep(1)
    conf23()
gpio.cleanup()
